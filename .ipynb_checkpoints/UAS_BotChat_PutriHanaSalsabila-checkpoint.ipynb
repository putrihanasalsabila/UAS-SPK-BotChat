    "import re\n",
    "import random\n",
    "\n",
    "sapaan = [\"Hai juga\", \"Halo juga\", \"Apa kabar\", \"Baik baik saja\"]\n",
    "\n",
    "while True:\n",
    "    x = input(\"User\\t:\")\n",
    "    if re.findall(r'hallo|hai|kabarmu', x):\n",
    "        print(\"Bot\\t:\", random.choice(sapaan))\n",
    "    else:\n",
    "        print(\"Bot/t: Aku tidak paham\")"
